"sign(-6.5) # Devuelve -1.0\n"
"sign(0.0) # Devuelve 0.0\n"
"sign(6.5) # Devuelve 1.0\n"
"signf(NAN)  # Devuelve 0.0\n"
"[/codeblock]"

