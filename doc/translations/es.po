"var dict = str_to_var(data)     # dict is a Dictionary\n"
"print(dict[\"a\"])                # Prints 1\n"
"[/gdscript]\n"
"[csharp]\n"
"string data = \"{ \\\"a\\\": 1, \\\"b\\\": 2 }\";           // data is a "
"string\n"
"var dict = GD.StrToVar(data).AsGodotDictionary(); // dict is a Dictionary\n"
"GD.Print(dict[\"a\"]);                              // Prints 1\n"
"[/csharp]\n"
"[/codeblocks]"
