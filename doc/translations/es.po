"from returning [constant @GDScript.NAN].\n"
"[codeblock]\n"
"var a = acosh(2) # Returns 1.31695789692482\n"
"cosh(a) # Returns 2\n"
"\n"
"var b = acosh(-1) # Returns 0\n"
"[/codeblock]"
msgstr ""
