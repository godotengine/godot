"[codeblock]\n"
"ProjectSettings.set(\"category/property_name\", 0)\n"
"\n"
"var property_info = {\n"
"    \"name\": \"category/property_name\",\n"
"    \"type\": TYPE_INT,\n"
"    \"hint\": PROPERTY_HINT_ENUM,\n"
"    \"hint_string\": \"one,two,three\"\n"
