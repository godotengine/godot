#!/usr/bin/env python

Import("env")
Import("env_modules")

env_csg = env_modules.Clone()

# Godot source files
env_csg.add_source_files(env.modules_sources, "*.cpp")
env_csg.add_source_files(env.modules_sources, "geometry_parser/*.cpp")
if env.editor_build:
    env_csg.add_source_files(env.modules_sources, "editor/*.cpp")
