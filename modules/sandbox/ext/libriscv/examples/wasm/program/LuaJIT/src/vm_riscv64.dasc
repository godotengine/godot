  |  addiw RD, NARGS8:RC, 8		// Compute (nresults+1)*8.
  |  addi TMP1, BASE, 8
  |  add TMP2, RA, RD
  |  sd CARG1, 0(RA)
  |  bne BASE, TMP2, >1
  |  j ->fff_res		// Done if exactly 1 argument.
  |1:
  |  ld TMP0, 0(TMP1)
  |  sd TMP0, -16(TMP1)
  |  mv TMP3, TMP1
  |  addi TMP1, TMP1, 8
  |  bne TMP3, TMP2, <1
  |  j ->fff_res
