  |  sltiu TMP3, TMP2, LJ_TISNUM
  |  neg TMP4, TMP3
  |  xor TMP0, TMP2, TISNUM		// TMP2 = TMP3 ? TISNUM : TMP2
  |  and TMP0, TMP0, TMP4
  |  xor TMP2, TMP0, TMP2
  |  slli TMP2, TMP2, 3
  |   sub TMP0, GL, TMP2
  |   ld TAB:RB, (offsetof(global_State, gcroot[GCROOT_BASEMT])-8)(TMP0)
