  |// Inlined GC threshold check.
  |.macro ffgccheck
  |   ld TMP0, GL->gc.total
  |   ld TMP1, GL->gc.threshold
  |  bltu TMP0, TMP1, >1
  |  jal ->fff_gcstep
  |1:
  |.endmacro
