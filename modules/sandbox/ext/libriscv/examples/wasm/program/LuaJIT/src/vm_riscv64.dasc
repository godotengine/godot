  |  ld CARG2, 0(RA)
  |  fld FARG2, 0(RA)
  |   bxgeu RA, RB, ->fff_resn
  |  checknum CARG2, >7
  |6:
  |  fpins FARG1, FARG1, FARG2
  |   addi RA, RA, 8
  |  j <5
  |7:  // Convert integer to number and continue above.
  |  checkint CARG2, ->fff_fallback
  |   fcvt.d.w FARG2, CARG2
  |  j <6
  |.endmacro
