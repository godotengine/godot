  |  bxnez TMP0, ->fff_fallback
  |  gettp TMP1, CARG1
  |   gettp TMP2, CARG2
  |  sltiu TMP1, TMP1, LJ_TISNUM
  |   sltiu TMP2, TMP2, LJ_TISNUM
  |  fld FARG1, 0(BASE)
  |  and TMP1, TMP1, TMP2
  |   fld FARG2, 8(BASE)
  |  bxeqz TMP1, ->fff_fallback
