  |.define NEXT_TAB,		TAB:CARG1
  |.define NEXT_IDX,		CARG2d
  |.define NEXT_IDXa,		CARG2
  |.define NEXT_PTR,		RC
  |.define NEXT_PTRd,		RCd
  |.define NEXT_TMP,		CARG3
