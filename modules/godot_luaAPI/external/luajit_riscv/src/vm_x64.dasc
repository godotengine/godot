  |  sub NEXT_IDX, NEXT_ASIZE
  |6:
  |  cmp NEXT_IDX, NEXT_TAB->hmask; ja >9
  |  imul NEXT_PTRd, NEXT_IDX, #NODE
  |  add NODE:NEXT_PTR, NEXT_TAB->node
  |  cmp qword NODE:NEXT_PTR->val, LJ_TNIL; je >7
  |  NEXT_RES_IDXL NEXT_ASIZE+1
